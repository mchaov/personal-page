{
    "name": "mchaov personal page",
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "Cache-Control": "max-age=31536000",
                "Link": "</code.css>; rel=preload; as=style, </code.js>; rel=preload; as=script"
            },
            "continue": true
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/about",
            "dest": "/about.html"
        }
    ]
}