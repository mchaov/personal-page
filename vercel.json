{
    "name": "mchaov personal blog",
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "Cache-Control": "max-age=31536000",
                "Link": "</main.css>; rel=preload; as=style"
            },
            "continue": true
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
                "src": "/and-one-more-test-article",
                "dest": "/and-one-more-test-article.html"
            },{
                "src": "/another-test-article",
                "dest": "/another-test-article.html"
            },{
                "src": "/test-article",
                "dest": "/test-article.html"
            }
    ]
}